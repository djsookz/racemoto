<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="#FFFFFF33">
    <item>
        <selector>
            <item android:state_pressed="true">
                <shape android:shape="rectangle">
                    <gradient
                        android:startColor="#5A52D5"
                        android:endColor="#4A42C5"
                        android:angle="45"/>
                    <corners />
                </shape>
            </item>
            <item>
                <shape android:shape="rectangle">
                    <gradient
                        android:startColor="#6C63FF"
                        android:endColor="#5A52D5"
                        android:angle="45"/>
                    <corners />
                </shape>
            </item>
        </selector>
    </item>
</ripple>